<script>

function myFunky(){
	let myTemp = document.getElementById('myText01').value
	let mySum = ' '
	document.getElementById('myDiv01').innerHTML = ' '

	myArray = new Array()
	myArray[0] = 'fred'
	myArray[1] = 'Mary'
	myArray[2] = 'Jim'
	myArray[3] = 'jom'
	myArray[4] = 'Jum'
	myArray[56] = 'im not creative enough to make a bunch of names'

	
	for (let myLoop=0; myLoop < myTemp; myLoop++){
		mySum += 'Loop #: ' + myLoop + ' array entry: ' + myArray[myLoop] +'<br>'
	}
	document.getElementById('myDiv01').innerHTML = mySum
}

</script>

<input type=text id="myText01" value="3"><br>

<input type=button value="cool array with 3 names" onmouseover="{
	myFunky()
}">

<div id="myDiv01">if you have the time try up to 6</div>
